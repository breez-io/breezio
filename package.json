  "scripts": {
    "test-nyc-strict": "cross-env NODE_ENV=TEST nyc --skip-full mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts' && nyc report --reporter=text-lcov > coverage/nyc-strict.log",
    "test-nyc": "nyc --skip-full mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts' && nyc report --reporter=text-lcov > coverage/nyc.log",
    "test-karma": "karma start",
    "build-less": "lessc --js src/editor/antd.less dist/antd.css && lessc --js src/editor/editor.less dist/editor.css",
    "build-icons": "ts-node tool/build-icons",
    "build-i18n": "ts-node tool/merge-lng",
    "build-package": "ts-node tool/build-package.ts",
    "test": "cross-env NODE_ENV=TEST mocha 'src/core/**/*.spec.ts' 'src/node/**/*.spec.ts'",
    "express-server": "node -r ts-node/register example/server/express-server",
    "editor": "parcel example/simple-editor/editor-browser.html --open --out-dir temp --no-source-maps",
    "server-and-editor-browser": "parcel example/simple-editor/server-and-editor-browser.html --open --out-dir temp --no-source-maps",
    "browser-server": "parcel example/simple-editor/server.html --open --out-dir temp --no-source-maps",
    "prettier-check": "prettier --check \"src/**/*.+(ts|tsx|less)\"",
    "prettier-write": "prettier --write \"src/**/*.+(ts|tsx|less)\""
  }
