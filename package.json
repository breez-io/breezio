  "scripts": {
    "nyc-cache": "nyc -s --nycrc-path cache.nycrc mocha src/common/**/*.spec.ts && mkdir coverage",
    "test-nyc-strict": "cross-env NODE_ENV=TEST nyc mocha src/common/**/*.spec.ts && nyc report --reporter=text-lcov > coverage/nyc-strict.log",
    "test-nyc": "nyc mocha src/common/**/*.spec.ts && nyc report --reporter=text-lcov > coverage/nyc.log",
    "test-karma": "karma start",
